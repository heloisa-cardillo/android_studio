<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity"
    android:background="@color/black">

    <TextView
        android:id="@+id/Mywork"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="184dp"
        android:gravity="center"
        android:text="@string/my_work"
        android:textColor="@color/white"
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"/>

    <TextView
        android:id="@+id/home"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        android:layout_marginTop="60dp"
        android:gravity="center"
        android:text="@string/home"
        android:textColor="@color/white"
        android:textSize="30sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <androidx.appcompat.widget.AppCompatButton
        android:id="@+id/button"
        android:layout_width="0dp"
        android:layout_height="40dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/shape_buttom"
        android:paddingStart="16dp"
        android:text="@string/search_github"
        android:textAlignment="textStart"
        android:textAllCaps="false"
        android:textColor="@color/gray"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="@id/home"
        app:layout_constraintTop_toBottomOf="@id/home"
        tools:ignore="RtlSymmetry" />


    <LinearLayout
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        android:layout_marginStart="16dp"
        android:layout_marginEnd="16dp"
        app:layout_constraintTop_toBottomOf="@id/Mywork"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginTop="16dp"
        android:background="@drawable/blacksquare">
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:id="@+id/issues_container"
            android:layout_height="50dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:focusable="true">
            <ImageView
                android:id="@+id/bissues"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@drawable/backgroundbottom1"
                android:src="@drawable/vector1"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="12dp"
                android:scaleType="centerInside"
                android:contentDescription="@string/bissues" />
            <TextView
                android:id="@+id/Issues"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="@string/Issues"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@id/bissues"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/arrow1"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:scaleType="centerInside"
                android:layout_marginEnd="8dp"
                android:src="@drawable/arrow"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <View
                android:layout_width="0dp"
                android:layout_height="1dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/Issues"
                android:id="@+id/ligne1"
                android:background="@color/lignes"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/pullrequest_container"
            android:focusable="true">
            <ImageView
                android:id="@+id/bpullrequest"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@drawable/backgroundbottom2"
                android:src="@drawable/vector2"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="12dp"
                android:scaleType="centerInside"
                android:contentDescription="@string/image_pullrequest" />
            <TextView
                android:id="@+id/pullrequest"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="PullRequest"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@id/bpullrequest"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/arrow2"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:scaleType="centerInside"
                android:layout_marginEnd="8dp"
                android:src="@drawable/arrow"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <View
                android:layout_width="0dp"
                android:layout_height="1dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/pullrequest"
                android:id="@+id/ligne2"
                android:background="@color/lignes"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/discussions_container"
            android:focusable="true">
            <ImageView
                android:id="@+id/bdiscussions"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@drawable/backgroundbottom3"
                android:src="@drawable/vector3"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="12dp"
                android:scaleType="centerInside"/>
            <TextView
                android:id="@+id/discussions"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="Discussions"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@id/bdiscussions"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/arrow3"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:scaleType="centerInside"
                android:layout_marginEnd="8dp"
                android:src="@drawable/arrow"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <View
                android:layout_width="0dp"
                android:layout_height="1dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/discussions"
                android:id="@+id/ligne3"
                android:background="@color/lignes"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/repositories_container"
            android:focusable="true">
            <ImageView
                android:id="@+id/brepositories"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@drawable/backgroundbottom4"
                android:src="@drawable/vector4"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="12dp"
                android:scaleType="centerInside"/>
            <TextView
                android:id="@+id/repositories"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="Repositories"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@id/brepositories"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/arrow4"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:scaleType="centerInside"
                android:layout_marginEnd="8dp"
                android:src="@drawable/arrow"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <View
                android:layout_width="0dp"
                android:layout_height="1dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/repositories"
                android:id="@+id/ligne4"
                android:background="@color/lignes"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/organizations_container"
            android:focusable="true">
            <ImageView
                android:id="@+id/borganizations"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@drawable/backgroundbottom5"
                android:src="@drawable/vector5"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="12dp"
                android:scaleType="centerInside"/>
            <TextView
                android:id="@+id/organizations"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="Organizations"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@id/borganizations"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/arrow5"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:scaleType="centerInside"
                android:layout_marginEnd="8dp"
                android:src="@drawable/arrow"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
            <View
                android:layout_width="0dp"
                android:layout_height="1dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintStart_toStartOf="@id/organizations"
                android:id="@+id/ligne5"
                android:background="@color/lignes"/>
        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="50dp"
            android:background="?attr/selectableItemBackground"
            android:clickable="true"
            android:id="@+id/starred_container"
            android:focusable="true">
            <ImageView
                android:id="@+id/bstarred"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:background="@drawable/backgroundbottom6"
                android:src="@drawable/vector6"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                app:layout_constraintBottom_toBottomOf="parent"
                android:layout_marginStart="12dp"
                android:scaleType="centerInside"/>
            <TextView
                android:id="@+id/starred"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="12dp"
                android:text="Starred"
                android:textColor="@color/white"
                app:layout_constraintBottom_toBottomOf="parent"
                android:textSize="16sp"
                app:layout_constraintStart_toEndOf="@id/bstarred"
                app:layout_constraintTop_toTopOf="parent"/>
            <ImageView
                android:id="@+id/arrow6"
                android:layout_width="24dp"
                android:layout_height="24dp"
                android:scaleType="centerInside"
                android:layout_marginEnd="8dp"
                android:src="@drawable/arrow"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>
